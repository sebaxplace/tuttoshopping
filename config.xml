<?xml version="1.0" encoding="UTF-8" ?>
<widget xmls  	= "http://www.w3.org/ns/widgets"
	xmlns:gap 	= "http://phonegap.com/ns/1.0"
	id			= "com.tuttoshopping"
	version		= "1.1.8">

<name>Tuttoshopping</name>
<description>
Scopri gli imperdibili sconti della tua citt√† con Tuttoshopping su centri benessere, arredamento, ristoranti e tanto altro. Iscriviti subito!!!
</description>
<author href="http://www.xplacecompany.com/" email="sebastian@xplacecompany.com">
Sebastian Frassia
</author>

<preference name="orientation" value="portrait" />
<preference name="target-device" value="universal" />
<preference name="prerendered-icon" value="true" />
<gap:plugin name="org.apache.cordova.device" version="0.2.12" />
<gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
<gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
<gap:plugin name="org.apache.cordova.media" version="0.2.13" />
<gap:plugin name="org.apache.cordova.vibration" version="0.3.11" />
<gap:plugin name="org.li8.inappbrowser" version="0.1" />
<gap:plugin name="nl.x-services.plugins.socialsharing" version="4.3.8" />
<gap:plugin name="com.pushwoosh.plugins.pushwoosh" source="plugins.cordova.io" />

<preference name="android-targetSdkVersion" value="12" />
<preference name="android-minSdkVersion" value="10" />
<gap:plugin name="com.phonegap.plugins.facebookconnect" version="0.9.0">
    <param name="APP_ID" value="287562268038826" />
    <param name="APP_NAME" value="TuttoShoppingSconti" />
</gap:plugin>

<access origin="*" />
<access origin="*.pushwoosh.com"/>
<access origin="http://127.0.0.1*" />
<access origin="https://m.facebook.com" subdomains="true"/>
<access origin="https://graph.facebook.com" subdomains="true"/>
<access origin="https://api.facebook.com" subdomains="true"/>
<access origin="https://*.fbcdn.net" subdomains="true"/>
<access origin="https://*.akamaihd.net" subdomains="true"/>
<access origin="geo:*" launch-external="yes"/>




